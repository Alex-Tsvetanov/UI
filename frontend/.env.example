# Frontend Environment Variables
# Copy this file to .env for local development
# Copy to .env.production for production builds

# Backend API URL (gRPC-Web endpoint)
# For local development: http://localhost:50051
# For Render production: https://domunity-backend.onrender.com
VITE_API_URL=http://localhost:50051

# Optional: Enable debug mode
# VITE_DEBUG=true

# Optional: API timeout (milliseconds)
# VITE_API_TIMEOUT=30000
